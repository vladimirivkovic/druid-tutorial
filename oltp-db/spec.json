{
    "type": "index_parallel",
    "spec": {
      "dataSchema": {
        "dataSource": "postgres_test",
        "timestampSpec": {
          "column": "startdate"
        },
        "dimensionsSpec": {
          "dimensions": [
            "workerid",
            "productid",
            "orderqty",
            "scrappedqty",
            "enddate",
            "duedate",
            "scrapreasonid",
            "modifieddate"
          ]
        },
        "metricsSpec": [
          {
            "type": "count",
                "name": "count"
              },
              {
                "type": "doubleSum",
                "name": "added",
                "fieldName": "added"
              },
              {
                "type": "doubleSum",
                "name": "deleted",
                "fieldName": "deleted"
              },
              {
                "type": "doubleSum",
                "name": "delta",
                "fieldName": "delta"
              }
